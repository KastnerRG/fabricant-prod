{
    "metrics-addr": "0.0.0.0:9323",
    "log-driver": "loki",
    "log-opts": {
        "loki-url": "http://localhost:3100/loki/api/v1/push"
    },
    "builder": {
        "gc": {
            "enabled": true,
            "defaultKeepStorage": "512GB",
            "policy": [
                {
                    "keepStorage": "0",
                    "filter": [
                        "unused-for=2160h"
                    ]
                }
            ]
        }
    }
}
